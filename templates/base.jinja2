<html>
<head>
<title>Mailroom</title>
</head>
<body>
	<h1>Donations</h1>
	<nav>
		<a id="all" style="padding-right:8px" href="{{ url_for("all")}}">All Donations</a>
		<a id="create" style="padding-right:8px" href="{{ url_for("create")}}">Create Donations</a>
		<a id="query" style="padding-right:8px" href="{{ url_for("query")}}">Query Donations</a>
		{% if session.username %}
		<a id="logout" style="padding-right:8px" href="{{ url_for("logout") }}">{{ session.username }} - Sign Out</a>
		{% else %}
		<a id="login" style="padding-right:8px" href="{{ url_for("login")}}">Sign In</a>
		{% endif %}
	</nav>
	<h2>{% block subtitle %} {% endblock subtitle %}</h2>
	{% block content %} {% endblock content %}
</body>
</html>
